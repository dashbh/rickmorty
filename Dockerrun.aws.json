{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "bhadash1/client",
      "hostname": "client",
      "essential": false,
      "memory": 128,
      "links": ["api"]
    },
    {
      "name": "api",
      "image": "bhadash1/api",
      "hostname": "api",
      "essential": false,
      "memory": 128,
      "links": ["mongo"]
    },
    {
      "name": "mongo",
      "image": "bhadash1/mongo",
      "hostname": "mongo",
      "essential": true,
      "memory": 128
    }
  ]
}